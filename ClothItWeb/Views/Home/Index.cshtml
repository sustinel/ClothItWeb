    <!-- Page Title -->
    <title>Cloth It - Share the way you look !</title>

    @{
        var culture = System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLowerInvariant();
    }
    @helper selected(string c, string culture)
{
    if (c == culture)
    {
        @:checked="checked"
    }
}

    <!-- Meta Description -->
    <!-- Meta GOOGLE -->
    <meta name="description" content="Share the way you look!">
    <meta name="keywords" content="cloth app, clothit, iphone app, ios app, android app, apple store, play store, share clothes, be popular, fashion">
    <meta name="author" content="ClothIt" />

    <!-- Meta FACEBOOK -->
    <meta property="og:title" content="Cloth It - Share the way you look !" />
    <meta property="og:description" content=@Resource.ClothitDesc />
    <meta property="og:description" content="ClothIt es una red social que te mantiene al tanto de las últimas tendencias de la moda. Nuesto objetivo es que puedas recibir diferentes opiniones de la comunidad sobre lo que llevas puesto o recibir sugerencias sobre lo que podrías llevar." />
    <meta property="og:description" content="Cloth It is a social network that targets fashion and trends about how to get dressed. Our goal is to bring users a different way to search for clothes or allowing them to get opinions from the community about the way they dress" />

    <meta property="og:url" content="http://clothit.me" />
    <meta property="og:type" content="website" />
