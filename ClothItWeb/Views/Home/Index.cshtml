    <!--
    Request invite
    ==================================== -->

    <section id="invite" class="invite">
        <div class="container">
            <div class="row mb50">
                <div class="sec-title text-center mb50 wow fadeInDown animated" data-wow-duration="500ms">
                    <h2>@Resource.RequestInvite</h2>
                    <div class="devider"><i class="fa fa-diamond fa-lg"></i></div>
                </div>
                <div class="sec-sub-title text-center wow rubberBand animated" data-wow-duration="1000ms">
                    <p>@Resource.RequestInviteDesc</p>
                </div>

                @using (Html.BeginForm("Subscribe", "Home", FormMethod.Post, new { @class = "subscribe" }))
                {
                    <div class="row">
                        <div class="col-md-6 col-md-offset-3">
                            <div class="contact-form">
                                <div class="input-group email">
                                    <div class="input-field" style="width:100%">
                                        <input type="email" name="Email" style="position:static" id="subscribeemail" placeholder="Email" class="form-control">
                                        <input type="submit" value="&#8594;" id="subs">
                                    </div>
                                </div>
                            </div>
                            <em>@Resource.RequestInviteWarning</em>
                        </div>
                    </div>
                }
                <hr style="margin-top:100px">
            </div>
        </div>
    </section>

    <!--
    End Request Invite
    ==================================== -->

    <!--
    Contact Us
    ==================================== -->

    <section id="contact" class="contact">
        <div class="container">
            <div class="row mb50">

                <div class="sec-title text-center mb50 wow fadeInDown animated" data-wow-duration="500ms">
                    <h2>@Resource.StoreInterested</h2>
                    <div class="devider"><i class="fa fa-diamond fa-lg"></i></div>
                </div>

                <div class="sec-sub-title text-center wow rubberBand animated" data-wow-duration="1000ms">
                    <p>@Resource.StoreInterestedDesc</p>
                </div>

                <!-- contact address -->
                <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12 wow fadeInLeft animated" data-wow-duration="500ms">
                    <div class="contact-address">
                        <h3>@Resource.Address</h3>
                        <p>Avda. 8 de Octubre 2738</p>
                        <p>Montevideo, Uruguay</p>
                        <p>+(598) 94587666</p>
                    </div>
                </div>
                <!-- end contact address -->
                <!-- contact form -->


                <div class="col-lg-8 col-md-8 col-sm-7 col-xs-12 wow fadeInDown animated" data-wow-duration="500ms" data-wow-delay="300ms">
                    <div class="contact-form">
                        <h3>@Resource.ContactUs</h3>
                        @using (Html.BeginForm("ContactUs", "Home"))
                        {
                            <div class="input-group name-email">
                                <div class="input-field">
                                    <input type="text" name="Nombre" id="name" placeholder=@Resource.Name class="form-control">
                                </div>
                                <div class="input-field">
                                    <input type="email" name="Email" id="email" placeholder="Email" class="form-control">
                                </div>
                            </div>
                            <div class="input-group">
                                <textarea name="Mensaje" id="message" placeholder=@Resource.Message class="form-control"></textarea>
                            </div>
                            <div class="input-group">
                                <input type="submit" id="form-submit" class="pull-right" value=@Resource.Send>
                                <label class="input-group pull-left" id="messageSent"></label>
                            </div>
                        }
                    </div>

                </div>

                <!-- end contact form -->
                <!-- footer social links -->
                <div class="col-lg-1 col-md-1 col-sm-1 col-xs-12 wow fadeInRight animated" data-wow-duration="500ms" data-wow-delay="600ms">
                    <ul class="footer-social">
                        <li><a href="https://www.twitter.com/cloth_it"><i class="fa fa-twitter fa-2x"></i></a></li>
                        <li><a href="https://www.facebook.com/clothit"><i class="fa fa-facebook fa-2x"></i></a></li>
                    </ul>
                </div>
                <!-- end footer social links -->

            </div>
        </div>

        <!-- Google map -->
        <div id="map_canvas" class="wow bounceInDown animated" data-wow-duration="500ms"></div>
        <!-- End Google map -->

    </section>

    <!--
    End Contact Us
    ==================================== -->


    <footer id="footer" class="footer">
        <div class="container">
            <div class="row">

                <div class="col-md-3 col-sm-6 col-xs-12 wow fadeInUp animated" data-wow-duration="500ms">
                    <div class="footer-single">
                        <img src="img/footer-logo.png" alt="">
                        <p>@Resource.ClothitDesc</p>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12 wow fadeInUp animated" data-wow-duration="500ms" data-wow-delay="600ms">
                    <div class="footer-single">
                        <h6>@Resource.Explore</h6>
                        <ul>
                            <li><a href="#features">@Resource.Howto</a></li>
                            <li><a href="#team">@Resource.Team</a></li>
                            <li><a href="#work">@Resource.Preview</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12 wow fadeInUp animated" data-wow-duration="500ms" data-wow-delay="900ms">
                    <div class="footer-single">
                        <h6>@Resource.Support</h6>
                        <ul>
                            <li><a href="#">@Resource.ContactUs</a></li>

                        </ul>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12 wow fadeInUp animated" data-wow-duration="500ms" data-wow-delay="1200ms">
                    <div class="footer-single">
                        <h6>@Resource.LanguageChooser</h6>
                        @using (Html.BeginForm("SetCulture", "Home"))
                        {
                            <ul>
                                <li>
                                    <input name="culture" id="en-us" value="en-us" type="radio" @selected("en-us", culture) /> @Resource.English
                                </li>
                                <li>
                                    <input name="culture" id="es" value="es" type="radio" @selected("es", culture) /> @Resource.Spanish
                                </li>
                            </ul>
                        }
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <p class="copyright text-center">
